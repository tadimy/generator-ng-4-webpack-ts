    "string-replace-loader": "1.0.5",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "ts-node": "2.1.0",
    "tslib": "1.5.0",
    "tslint": "4.4.2",
    "typedoc": "0.5.5",
    "typescript": "2.1.6",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.0",
    "webpack-dev-server": "2.3.0",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "webpack-merge": "2.6.1"
  }
}
